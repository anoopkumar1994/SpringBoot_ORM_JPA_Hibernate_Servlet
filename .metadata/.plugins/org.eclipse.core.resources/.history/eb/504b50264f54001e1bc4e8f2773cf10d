package com.hibernetDemo.HibernateDemo;

import com.mysql.cj.Session;

/**
 * Hello world!
 *
 */
public class App 
{
    public static void main( String[] args )
    {
        Student st = new Student();
        st.setId(1);
        st.setName("Shyam");
        st.setRollNo(10);
        st.setMarks(150);
        
        Session session;
        session.save(st);
    }
}
